{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static" },
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["build/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "build/index.js"
    }
  ],
  "outputDirectory": "build"
}
