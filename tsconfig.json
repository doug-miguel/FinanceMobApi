{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node LTS",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "baseUrl": "/",
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true
  },
  "include": [
    "src/**/*",
    "index.ts"
  ]
}